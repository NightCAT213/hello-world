import discord

class MyClient(discord.Client):
    async def on_ready(self):
        print('Logged on as', self.user)

    async def on_message(self, message):
        # don't respond to ourselves
        if message.author == self.user:
            return

        if message.content == 'ping':
            await message.channel.send('pong')
        if message.content == 'pong':
            await message.channel.send('ping')
        if message.content == 'Привет':
            await message.channel.send('Привет!')
        if message.content == 'Привет!':
            await message.channel.send('Прив')
        if message.content == 'By':
            await message.channel.send('By')
        if message.content == 'By!':
            await message.channel.send('By!') 
        if message.content == 'Как дела':
            await message.channel.send('Норм. А у тебя?')
        if message.content == 'Как дела?':
            await message.channel.send('Хорошо. А ты как?')
        if message.content == 'Норм':
            await message.channel.send('Супер! Что собираешься делать?') 
        if message.content == 'Нормально':
            await message.channel.send('Ну и хорошо. Что делаешь?')
        if message.content == 'Уроки':
            await message.channel.send('Давай делай, делай. Я мысленно тебе помогаю)))') 
        if message.content == 'New, а как тебя зовут?':
            await message.channel.send('New, а как же еще!')
        if message.content == 'Ты кто?':
            await message.channel.send('Конь пекто и дед  пальто! Извини за грубость')
            
            

client = MyClient()
client.run('NzY4MTM2MzA2MDg1MTk5OTQz.X48Erg.a-zYuvSoPOB-NmyRJ3A6xpF9ofk')
